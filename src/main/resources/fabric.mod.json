{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "Server mod adding useful tools",
  "authors": ["${mod_author}"],
  "contact": {
    "sources": "https://github.com/${repository}",
    "issues": "https://github.com/${repository}/issues"
  },
  "license": "CC0-1.0",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.possible_triangle.polytools.datagen.DataGenerators"
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.possible_triangle.polytools.PolytoolsMod"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.15.7",
    "fabric": "*",
    "minecraft": "~1.20",
    "java": ">=17",
    "fabric-language-kotlin": "1.9.6+kotlin.1.8.22",
    "polymer-core": "*"
  },
  "suggests": {}
}
